        package tictactoe;

        import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt(); //22

        int magic = 0;
        while (n > 0) {
            n /= 10; // 2
            System.out.println("n " + n);
            magic++;
        }
        System.out.println("magic " + magic);
    }
}

